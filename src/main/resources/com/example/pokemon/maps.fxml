            prefHeight="400.0" prefWidth="600.0">


    <VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml">
        <Button text="返回" onAction="#goBack"/>
